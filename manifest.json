{
  "manifest_version": 3,
  "name": "equery",
  "version": "0",
  "description": "Search for horses by contextMenus and omnibox",
  "icons": {
    "128": "128.png"
  },
  "omnibox": {
    "keyword": "eq"
  },
  "permissions": [
    "contextMenus"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "bg.js"
  }
}